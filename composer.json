{
    "name": "qdd/es",
    "description": "yaf es",
    "keywords": [
        "elasticsearch",
        "php",
        "laravel",
        "lumen",
        "scout",
        "fulltext",
        "indexing",
        "builder"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "mucy0729",
            "email": "mucy0729@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qdd\\Es\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qdd\\Es\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.6",
        "elasticsearch/elasticsearch": "^5.0|^6.0",
        "illuminate/pagination": "*",
        "illuminate/support": "*",
        "symfony/var-dumper": "*",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qdd\\Es\\ElasticsearchServiceProvider"
            ],
            "aliases": {
                "ES": "Qdd\\Es\\Facades\\ES"
            }
        }
    }
}
